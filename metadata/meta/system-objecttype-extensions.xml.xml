<?xml version="1.0" encoding="UTF-8"?>
<metadata xmlns="http://www.demandware.com/xml/impex/metadata/2006-10-31">

    <type-extension type-id="SitePreferences">
        <custom-attribute-definitions>
            <attribute-definition attribute-id="fitGeniusDomain">
                <display-name xml:lang="x-default">Fit Genius Domain</display-name>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="fitGeniusFootProfile">
                <display-name xml:lang="x-default">Fit Genius Foot Profile</display-name>
                <description xml:lang="x-default">Toggle to display the foot profile on the Product Details Page.</description>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <default-value>false</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="fitGeniusFootScore">
                <display-name xml:lang="x-default">Fit Genius Foot Score</display-name>
                <description xml:lang="x-default">Toggle to display the foot score on the Product Listing Page.</description>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <default-value>false</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="fitGeniusProfile">
                <display-name xml:lang="x-default">Fit Genius Profile</display-name>
                <description xml:lang="x-default">Toggle to display the fit genius profile on the login page</description>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <default-value>false</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="fitGeniusScoreCategories">
                <display-name xml:lang="x-default">Fit Genius Score Categories</display-name>
                <description xml:lang="x-default">Used to define the categories on which the fit genius score is displayed</description>
                <type>set-of-string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
            </attribute-definition>
            <attribute-definition attribute-id="integrationMode">
                <display-name xml:lang="x-default">Integration Mode</display-name>
                <type>enum-of-string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <value-definitions>
                    <value-definition default="true">
                        <display xml:lang="x-default">Test</display>
                        <value>test</value>
                    </value-definition>
                    <value-definition>
                        <display xml:lang="x-default">Live</display>
                        <value>live</value>
                    </value-definition>
                </value-definitions>
            </attribute-definition>
            <attribute-definition attribute-id="sizeRightWidget">
                <display-name xml:lang="x-default">Size Right Widget</display-name>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <default-value>false</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="widgetFontColor">
                <display-name xml:lang="x-default">Widget Font Color</display-name>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="widgetButtonColor">
                <display-name xml:lang="x-default">Widget Button Color</display-name>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
        </custom-attribute-definitions>
        <group-definitions>
            <attribute-group group-id="fitGeniusWidgets">
                <display-name xml:lang="x-default">Fit Genius Widgets</display-name>
                <attribute attribute-id="fitGeniusScoreCategories"/>
                <attribute attribute-id="fitGeniusFootScore"/>
                <attribute attribute-id="fitGeniusProfile"/>
                <attribute attribute-id="fitGeniusFootProfile"/>
                <attribute attribute-id="fitGeniusDomain"/>
                <attribute attribute-id="widgetButtonColor"/>
                <attribute attribute-id="widgetFontColor"/>
                <attribute attribute-id="sizeRightWidget"/>
                <attribute attribute-id="integrationMode"/>
            </attribute-group>
        </group-definitions>
    </type-extension>
</metadata>
